{
  "name": "@mbaxter/prediction-market",
  "version": "0.0.0",
  "description": "Solidity practice project",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "app-serve": "$(npm bin)/http-server public -p 8080",
    "app-build": "$(npm bin)/webpack",
    "app-watch": "$(npm bin)/webpack --watch",
    "contract-compile": "$(npm bin)/truffle compile",
    "contract-deploy": "$(npm bin)/truffle migrate --network net42 --reset",
    "contract-test": "npm run testrpc > /dev/null & truffle test; npm run testrpc-kill",
    "lint": "$(npm bin)/eslint {test, test-util, app/src}",
    "test": "npm run contract-test",
    "testrpc": "$(npm bin)/testrpc -a 10 --network-id=42",
    "testrpc-kill": "kill $(ps aux | grep '[t]estrpc' | awk '{print $2}')"
  },
  "author": "mbaxter",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-loader": "6.2.7",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-bluebird": "0.1.1",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "chai": "4.1.1",
    "css-loader": "0.25.0",
    "eslint": "4.5.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-react": "6.6.0",
    "ethereumjs-testrpc": "4.1.1",
    "ethereumjs-util": "^5.1.2",
    "http-server": "^0.10.0",
    "json-loader": "0.5.7",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "postcss-loader": "1.1.1",
    "promise-mysql": "2.0.0",
    "redux-logger": "2.7.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.3"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "bluebird": "^3.5.0",
    "co": "^4.6.0",
    "co-parallel": "1.0.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "reselect": "^2.5.4",
    "truffle": "3.4.9",
    "truffle-contract": "2.0.5",
    "web3": "^0.20.1"
  }
}
